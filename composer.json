{
  "name": "cashforapp/helpers",
  "type": "library",
  "description": "Assistant package",
  "license": "GPL-3.0",
  "minimum-stability": "stable",
  "homepage": "https://github.com/cashforapp/helpers",
  "authors": [
    {
      "name": "Sergey Andrianov",
      "email": "batisska@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-json": "*"
  },
  "require-dev": {
    "mockery/mockery": "^1.4.0",
    "orchestra/testbench": "^6.0",
    "laravel/legacy-factories": "^1.0.4",
    "phpstan/phpstan": "^0.12.71",
    "friendsofphp/php-cs-fixer": "^2.18",
    "phpstan/phpstan-mockery": "^0.12.12",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan-beberlei-assert": "^0.12.6",
    "nunomaduro/larastan": "^0.7.0",
    "phpstan/phpstan-phpunit": "^0.12.17",
    "barryvdh/laravel-ide-helper": "^2.9"
  },
  "autoload": {
    "psr-4": {
      "PhpCraftsman\\": "src/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/",
      "Database\\Migrations\\": "database/migrations/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
